{
  "name": "TDEM",
  "version": "1.1.1",
  "description": "Install, search and manage extensions for TweetDeck",
  "author": "Ben Dixon <me@nuro.sh>(https://nuro.sh)",
  "license": "MIT",
  "main": "src/manifest.json",
  "scripts": {
    "build": "rimraf .cache/ dist/ && parcel build src/manifest.json --no-source-maps --no-cache --log-level 3",
    "dev": "rimraf .cache/ dist/ && parcel watch src/manifest.json --no-source-maps --no-cache --log-level 3",
    "package": "npm run build && crx pack dist -o TDEM.crx",
    "lint": "prettier --config .prettierrc.json --write '**/*.{js,ts,vue}'"
  },
  "staticPath": {
    "source": [
      "/src/fonts"
    ],
    "target": [
      "/dist/fonts"
    ]
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1",
    "vuetify": "^1.3.11"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "crx": "^3.2.1",
    "json5": "^2.1.0",
    "parcel-plugin-clean-dist": "^0.0.6",
    "parcel-plugin-copy": "^1.0.2",
    "parcel-plugin-web-extension": "^1.4.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "vue-template-compiler": "^2.5.17"
  }
}
